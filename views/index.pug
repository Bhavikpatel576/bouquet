                    ['Tokyo', 9.0],
                    ['Cairo', 8.9],
                    ['Dhaka', 8.9],
                    ['Mexico City', 8.9],
                    ['Lima', 8.9]
              ]
              }];
           
              var json = {};   
              json.chart = chart; 
              json.title = title;   
              json.subtitle = subtitle; 
              json.tooltip = tooltip;
              json.xAxis = xAxis;
              json.yAxis = yAxis;  
              json.series = series;
              json.plotOptions = plotOptions;  
              json.credits = credits;
              $('#container').highcharts(json);
           });
          }) 
